<h1>Registrate</h1>

<%# render "form", user: @user %> 

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Registrarse"%>
<% end %>

<br>

<div>
  <%= link_to "Back to users", users_path %>
</div>
